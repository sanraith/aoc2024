{
    "liveServer.settings.root": "/dist",
    "files.exclude": {
        "**/.bloop": true,
        "**/.bsp": true,
        "**/.metals": true,
        "**/.scala-build": true,
        "**/node_modules": true,
        "**/target": true,
        "project/project": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true
    },
    "files.watcherExclude": {
        "**/target": true
    },
    "cSpell.ignorePaths": [
        "build.sbt",
        "Day*Test.scala",
        "package-lock.json",
        "node_modules",
        "vscode-extension",
        ".git/{info,lfs,logs,refs,objects}/**",
        ".git/{index,*refs,*HEAD}",
        ".vscode",
        ".vscode-insiders"
    ],
    "cSpell.words": [
        "Collinearity",
        "DIRPAD",
        "dont",
        "Fragmenter",
        "scalajs",
        "vite"
    ],
}